10 SCREEN 8,3
20 COLOR 15,0,0:CLS
30 SPRITE$(0)=STRING$(32,255)
40 T=0
50 FOR J = 0 TO 3
60 FOR I = 0 TO 3
70 PUT SPRITE T  ,(I*48,J*40),8,0
80 PUT SPRITE T+1,(I*48+8,J*40),64+2,0
90 T=T+2
100 NEXT
110 NEXT
120 T=0
130 FOR I=0 TO 15
140 COLOR SPRITE$(T)  =STRING$(8,T)+STRING$(8,64+T)
150 COLOR SPRITE$(T+1)=STRING$(8,T+1+64)+STRING$(8,T+1)
160 T=T+2
170 NEXT
180 REM GOTO 180
